<h3>Nova oferta</h3>

<%= form_for @deal, :url => admin_generic_deals_path(@deal) do |f| %>
  <%=  render :partial => 'form', :locals => {:f => f} %>
  <div class="actions"><%= f.submit 'Criar' , :class => "button" %></div>
<% end %>

<p><%= link_to "Voltar", admin_generic_deals_path, :class => "button" %></p>